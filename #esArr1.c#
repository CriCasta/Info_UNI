61;7600;1c#include <stdio.h>
#define D 10

int main(){
	int a[D];
	int i, sum;

	for(int i=0;i<D;i++){
		scanf("%d",&a[i]);
	}
	printf("\nArray: ");
	for(int i=0;i<D;i++){
               printf("%d\n",a[i]);
        }
	sum=0;
	for(int i=0;i<D;i++){
               sum=sum+a[i];
        }
	printf("%d\n",sum);
	return 0;
}
